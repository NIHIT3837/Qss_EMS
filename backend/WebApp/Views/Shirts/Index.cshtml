@model List<Shirt>

	<h3>SHIRTS</h3>

	<br/>
<table class="table table-dark table-striped">

	<thead>
		<tr>
			<td>ID</td>
			<td>Brand</td>
			<td>Gender</td>
			<td>Color</td>
			<td>Size</td>
			<td>Price</td>
		</tr>
	</thead>

	<tbody>
		@foreach(var shirt in Model)
		{
			<tr>
				<td>@shirt.ShirtId</td>
				<td>@shirt.Brand</td>
				<td>@shirt.Gender</td>
				<td>@shirt.Color</td>
				<td>@shirt.Size</td>
				<td>@shirt.Price</td>
			</tr>
		}
	</tbody>
	...
</table>